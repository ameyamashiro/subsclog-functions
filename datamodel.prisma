type Link {
  id: ID! @unique
  author: String
  content: String
  description: String
  publishedAt: DateTime!
  sourceId: String
  sourceName: String
  title: String!
  url: String! @unique
}

type Comment {
  id: ID! @unique
  createdAt: DateTime!
  updatedAt: DateTime!
  text: String!
  service: Service!
  ownerId: String
}

type Review {
  id: ID! @unique
  createdAt: DateTime!
  updatedAt: DateTime!
  rate: Int
  rating: Int!
  text: String!
  serviceId: String!
  service: Service!
  ownerId: String
}

type Category {
  id: ID! @unique
  createdAt: DateTime!
  updatedAt: DateTime!
  name: String!
  services: [Service!]!
  shortDescription: String!
  description: String!
}

type Service {
  address: String
  category: Category
  createdAt: DateTime!
  description: String!
  freetrial: String
  hashtag: String
  id: ID! @unique
  inquiry: String
  isOnline: Boolean @default(value: false)
  likeCount: Int! @default(value: 0)
  multiplans: String
  note: String
  phonenumber: String
  photoURL: String
  place: String
  price: Float
  rating: Int @default(value: 0)
  ratingCount: Int @default(value: 0)
  reviews: [Review!]!
  tagline: String
  title: String!
  updatedAt: DateTime!
  url: String
  viewCount: Int! @default(value: 0)
  yomigana: String
}
